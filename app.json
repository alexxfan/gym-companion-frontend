{
  "expo": {
    "name": "gym-companion-frontend",
    "slug": "gym-companion-frontend",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/gymicon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.gymcompanion.app",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/gymicon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.gymcompanion.app"
    
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/gymicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/gymicon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "expo-secure-store"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "85cbebfc-97aa-41e2-95c4-b34bf6c10e46"
      }
    }
  }
}
